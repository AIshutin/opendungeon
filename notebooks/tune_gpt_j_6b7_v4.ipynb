        "model_wrapped_for_trainer = MyModelWrapperThatSaves(model)\n",
        "state_dict_adapters_only = model_wrapped_for_trainer.state_dict()\n",
        "model_wrapped_for_trainer.load_state_dict(state_dict_adapters_only)\n",
